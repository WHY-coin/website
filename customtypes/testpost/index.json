{
  "format": "page",
  "id": "testpost",
  "label": "BlogPost",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "text": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "SEO & Metadata": {
      "meta_title": {
        "config": {
          "label": "Meta Title",
          "placeholder": "A title of the page used for social media and search engines"
        },
        "type": "Text"
      },
      "meta_description": {
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        },
        "type": "Text"
      },
      "meta_keywords": {
        "type": "Text",
        "config": {
          "label": "META Keywords",
          "placeholder": ""
        }
      },
      "meta_image": {
        "config": {
          "constraint": {
            "height": 1260,
            "width": 2400
          },
          "label": "Meta Image",
          "thumbnails": []
        },
        "type": "Image"
      },
      "source": {
        "type": "Link",
        "config": {
          "label": "Source",
          "placeholder": "Source of post",
          "select": null,
          "allowTargetBlank": true,
          "allowText": true,
          "repeat": false
        }
      }
    }
  }
}
